{
    "editor.rulers": [80, 100],
    "go.lintTool": "golangci-lint",
    "gopls": {
        "local": "github.com/twelvelabs/stylist",
        "ui.semanticTokens": true
    },
    "cSpell.words": [
        "bmatcuk",
        "Debugf",
        "Debugln",
        "deckarep",
        "doublestar",
        "errgroup",
        "gjson",
        "globbing",
        "gofmt",
        "golangci",
        "iostreams",
        "ireturn",
        "logrus",
        "mapset",
        "pflag",
        "renderable",
        "shellcheck",
        "shlex",
        "sirupsen",
        "stretchr",
        "tidwall",
        "unparam"
    ]
}
